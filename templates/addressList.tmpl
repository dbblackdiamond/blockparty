<!doctype html>
<html>
<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1"><meta charset="UTF-8">
		<link href="/css/bootstrap.min.css" rel="stylesheet"/>
		<link href="/css/blockparty.css" rel="stylesheet"/>
		<link href="/css/jquery-ui.min.css" rel="stylesheet"/>
		<script type="text/javascript" src="/js/web3.js"></script>
		<script type="text/javascript" src="/js/jquery.min.js"></script>
		<script type="text/javascript" src="/js/bootstrap.min.js"></script>
		<script type="text/javascript" src="/js/d3.min.js"></script>
		<script type="text/javascript" src="/js/blockparty.js"></script>
		<script type="text/javascript" src="/js/jquery-ui.min.js"></script>
		<script type="text/javascript" src="/js/waitingfor.js"></script>
		<title>BlockParty</title>
</head>
    <body>
	<div class="container-fluid">
		<h2 class="text-center" id="title">Addresses</h2>
		<div>
			<table class="table table-striped">
			<thead>
				<tr>
					<th>Address</th>
					<th>User ID</th>
					<th>Status</th>
				</tr>
			</thead>
			<tbody id="tableData">
			{{range .Data}}
				<tr>
					<td>{{.ID}}</td>
					<td>{{.UserID}}</td>
					<td>{{.Status}}
				</tr>
			{{end}}
			</tbody>
			</table>
		</div>
	</div>
    </body>
</html>
